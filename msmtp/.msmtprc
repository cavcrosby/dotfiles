# set default values for all following accounts
defaults
auth           on
tls            on
tls_trust_file /etc/ssl/certs/ca-certificates.crt
logfile        ~/.msmtp.log

# gmail
account        gmail
host           smtp.gmail.com
port           587
from           cavcrosby@gmail.com
user           cavcrosby@gmail.com
# password is to be set in ~/.netrc

# set a default account
account default : gmail
